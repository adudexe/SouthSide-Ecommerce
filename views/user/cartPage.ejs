    <script>
        document.addEventListener("DOMContentLoaded",()=>{
            const Toast = Swal.mixin({
                toast: true,
                position: "top-end",
                showConfirmButton: false,
                timer: 3000,
                timerProgressBar: true,
                didOpen: (toast) => {
                    toast.onmouseenter = Swal.stopTimer;
                    toast.onmouseleave = Swal.resumeTimer;
                }
            });

            document.querySelectorAll(".cartDetails").forEach((container)=>{
                //since the cart details will be in different container so an listene should be there for all the container's
                const qtyUp = container.querySelector(".qty-up");
                const qtyDown = container.querySelector(".qty-down");
                const quantity = container.querySelector(".qty-val");
                const remove = container.querySelector(".cart-remove")
                
                
                // let currentQuantity = parseInt(quantity.textContent);
                // console.log(currentQuantity)

                qtyUp.addEventListener("click",async (e)=>{
                    e.preventDefault();
                    const productId = container.querySelector(".product-table-name").getAttribute("data-product-id");
                    //Getting product details from backend 
                    const variantId = container.querySelector(".product-table-name").getAttribute("data-variant-id");
                    //updating the variant details in the backend.

                    //To get the current quantity from the text document...
                    let currentQuantity = parseInt(quantity.textContent);
                    //assign the new quantity to a variable for checking and validation puropose..
                    let newQuantity = currentQuantity + 1;
                    //To store the new quantity in Java Script Object
                    let data = {};

                    //To check if the quantity has exceeded the maximum number..
                    if(newQuantity>=5)
                    {
                        Toast.fire({
                        icon: "error",
                        title: "Max Quantity Reached.."
                        });
                    }
                    data.newQuantity = newQuantity;
                    data.productId = productId;
                    data.variantId = variantId;

                    let response = await fetch("/user/cart/quantityinc",{
                        method:"PATCH",
                        headers:{
                            "Content-Type":"application/json"
                        },
                        body:JSON.stringify(data)
                    })
                    console.log(response)
                    let details = await response.json();
                    console.log(details)
                    if(details.success)
                    {
                        document.getElementById("sub-total").innerHTML =""
                            document.getElementById("sub-total").innerHTML=`
                                <h6>Subtotal</h6>
                                <h6 class="text-brand" >₹ ${details.totalPrice+80}</h6>
                            `
                            // document.getElementById("discount").innerHTML = ""
                            // document.getElementById("discount").innerHTML = `
                            //         <h6>Product Offer Discount</h6>
                            //         <h6 class="text-brand">-${details.discount}</h6>
                            // `

                            

                            document.getElementById("totalPrice").innerHTML = ""
                            document.getElementById("totalPrice").innerHTML = `
                                <h5 class="font-lg">Total</h5>
                                <h5 class="font-lg text-brand">₹ ${details.totalPrice}</h5>
                            `
                        // Toast.fire({
                        // icon: "error",
                        // title: "Max Quantity Reached.."
                        // });
                        quantity.textContent = newQuantity
                    }
                    else
                    {
                        Toast.fire({
                        icon: "error",
                        title: details.message || "Internal Server Error.."
                        });
                    }
                })

                qtyDown.addEventListener("click",async (e)=>{
                    
                    e.preventDefault();

                        const productId = container.querySelector(".product-table-name").getAttribute("data-product-id");
                        //Getting product details from backend 
                        const variantId = container.querySelector(".product-table-name").getAttribute("data-variant-id");
                        //updating the variant details in the backend.
    
                        //To get the current quantity from the text document...
                        let currentQuantity = parseInt(quantity.textContent);
                        //assign the new quantity to a variable for checking and validation puropose..
                        let newQuantity = currentQuantity - 1;
                        //To store the new quantity in Java Script Object
                        let data = {};
    
                        //To check if the quantity has exceeded the maximum number..
                        if(newQuantity>=5)
                        {
                            Toast.fire({
                            icon: "error",
                            title: "Max Quantity Reached.."
                            });
                        }
                        data.newQuantity = newQuantity;
                        data.productId = productId;
                        data.variantId = variantId;
    
                        let response = await fetch("/user/cart/quantitydec",{
                            method:"PATCH",
                            headers:{
                                "Content-Type":"application/json"
                            },
                            body:JSON.stringify(data)
                        })
                        console.log(response)
                        let details = await response.json();
                        console.log(details)
                        if(details.success)
                        {
                            // Toast.fire({
                            // icon: "error",
                            // title: "Max Quantity Reached.."
                            // });
                            quantity.textContent = newQuantity;
                            document.getElementById("sub-total").innerHTML =""
                            document.getElementById("sub-total").innerHTML=`
                                <h6>Subtotal</h6>
                                <h6 class="text-brand" >₹ ${details.totalPrice+80}</h6>
                            `
                            // document.getElementById("discount").innerHTML = ""
                            // document.getElementById("discount").innerHTML = `
                            //         <h6>Product Offer Discount</h6>
                            //         <h6 class="text-brand">-${details.discount}</h6>
                            // `

                            document.getElementById("totalPrice").innerHTML = ""
                            document.getElementById("totalPrice").innerHTML = `
                                <h5 class="font-lg">Total</h5>
                                <h5 class="font-lg text-brand">₹ ${details.totalPrice}</h5>
                            `


                        }
                        else
                        {
                            Toast.fire({
                            icon: "error",
                            title: details.message || "Internal Server Error.."
                            });
                        }  
                    })
                
                remove.addEventListener("click",async (e)=>{
                    const productId = container.querySelector(".product-table-name").getAttribute("data-product-id");
                    //Getting product details from backend 
                    const variantId = container.querySelector(".product-table-name").getAttribute("data-variant-id");
                    //updating the variant details in the backend.

                    const data = {} 
                    data.productId = productId;
                    data.variantId = variantId;

                    let response = await fetch("/user/cart/delete/",{
                        method:"DELETE",
                        headers:{
                            "Content-Type":"application/json"
                        },
                        body:JSON.stringify(data)
                    })

                    console.log(response);

                    let details = await response.json();

                    console.log(details);
                    if(details.success)
                    {
                        window.location.reload();
                    }
                    else
                    {
                        Toast.fire({
                        icon: "error",
                        title: details.message || "Internal Server Error.."
                        });                               
                    }

                })
                
            })

            

                
        })
            
        

    </script>
