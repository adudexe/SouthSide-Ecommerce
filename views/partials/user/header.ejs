    /* Styling for the user icon and dropdown */
    .user-icon {
        display: flex;
        align-items: center;
        position: relative;
        cursor: pointer;
        padding: 10px;
    }

    .user-icon:hover {
        background-color: #f0f0f0;
        border-radius: 50%;
    }

    .user-avatar {
        width: 32px;
        height: 32px;
        border-radius: 50%;
        object-fit: cover;
        margin-right: 8px;
    }

    .user-name {
        font-weight: bold;
        color: #333;
        font-size: 14px;
        line-height: 1;
    }

    .dropdown-menu {
        border-radius: 8px;
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
        min-width: 180px;
        padding: 10px 0;
        background-color: #fff;
        position: absolute;
        top: 100%;
        right: 0;
        z-index: 9999;
        border: 1px solid #e0e0e0;
        display: none;
    }

    .dropdown:hover .dropdown-menu {
        display: block;
    }

    .dropdown-menu .dropdown-item {
        padding: 10px 15px;
        font-size: 14px;
        color: #555;
        border-bottom: 1px solid #f1f1f1;
        transition: background-color 0.3s ease;
    }

    .dropdown-menu .dropdown-item:hover {
        background-color: #f5f5f5;
    }

    .header-middle.header-middle-ptb-1.d-none.d-lg-block {
        border-bottom: none !important;
    }
    
    .header-style-1 .header-middle-ptb-1 {
        border-bottom: none !important;
    }
    
    .header-middle {
        border: none !important;
    }
    .order-details-page .header-middle,
    .order-details-page .header-middle-ptb-1,
    .order-details-page .header-style-1 .header-middle-ptb-1 {
        border: none !important;
        border-bottom: none !important;
    }
    </style>

